<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.moa.trip.mapper.ExpenseMapper">
    <insert id="insert" parameterType="org.moa.trip.entity.Expense" useGeneratedKeys="true" keyProperty="expenseId">
        INSERT INTO EXPENSE(trip_id,
                            member_id,
                            expense_name,
                            amount,
                            location,
                            settlement_completed,
--                             expense_date,
                            created_at,
                            updated_at
        )
        VALUES (#{tripId},
                #{memberId},
                #{expenseName},
                #{amount},
                #{location},
                #{settlementCompleted},
--                 #{expenseDate},
                #{createdAt},
                #{updatedAt}
        )
    </insert>
</mapper>
